@model CreateDiscountCouponDtos
@{
    ViewData["Title"] = "CreateDiscount";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>CreateDiscount</h1>
<div class="container">
    <form method="post" class="form-control">
        <label class="form-control">Kupon Adı</label>
        <input type="text" asp-for="Code" />
        <br />
        <label class="form-control">Kupon Oranı</label>
        <input type="text" asp-for="Rate" />
        <br />
        <button class="btn btn-success">Ekle</button>
    </form>
</div>

